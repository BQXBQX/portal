{
  "productName": "graphscope-portal",
  "version": "0.1.0",
  "identifier": "com.graphscope-portal.app",
  "build": {
    "beforeDevCommand": "npm run start",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build:site",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "graphscope-portal",
        "width": 1366,
        "height": 768
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/Square30x30Logo.png",
      "icons/Square310x310Logo.png",
      "icons/StoreLogo.png"
    ]
  }
}
