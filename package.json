{
  "engines": {
    "node": "^16",
    "pnpm": "^8"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build:all:es": "turbo run build:es",
    "build:all:umd": "turbo run build:umd --no-cache",
    "clean": "pnpm run -r clean",
    "docs": "NODE_OPTIONS=--max_old_space_size=4098 && dumi dev",
    "publish": "pnpm publish -r --registry=https://registry.npmjs.org"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "type": "module",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "buffer-browserify": "^0.2.5",
    "node-fetch": "^3.3.0",
    "react": "18.x",
    "react-dom": "18.x"
  },
  "devDependencies": {
    "@types/react": "18.x",
    "@types/react-dom": "18.x",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.8.1",
    "dumi": "^2.2.16",
    "dumi-theme-antd-style": "latest",
    "father": "^2.30.23",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "turbo": "^1.8.3",
    "typescript": "^5.1.6",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": []
}
