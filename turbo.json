{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build:esm": {
      "dependsOn": ["^build:esm"],
      "outputs": ["es/**"]
    },
    "build:cjs": {
      "dependsOn": ["^build:cjs"],
      "outputs": ["lib/**"]
    },
    "build:umd": {
      "dependsOn": ["^build:umd"],
      "outputs": ["dist/**"]
    },
    "sync": {
      "dependsOn": ["^sync"]
    }
  },
  "globalDependencies": [".prettierrc.js"]
}
